import random
import IPO
import sys

(P,mils,origObj) = IPO.readSCIP(sys.argv[1])

print P.dimension

for i in xrange(10):
  if i == 0:
    print('Original objective')
    objective = origObj
  else:
    print('Random objective #%d' % (i))
    objective = [ (random.random() - 0.5) for j in xrange(P.space.dimension) ]
  for facet in IPO.generateFacets(P, mils, objective):
    print facet
